    	dataSetInterval = setInterval(function() {
	        var timerFlag = '{!HTMLENCODE(DeploymentResultValue.Status__c)}';
	        if (timerFlag == 'Finished' || timerFlag == 'Timedout')//MD-2711
	            clearInterval(dataSetInterval);
	        else
	            refreshDataSet();
	    }, 5000);

	    return dataSetInterval;
    }

