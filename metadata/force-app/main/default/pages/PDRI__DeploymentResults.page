    function toggleElementVisibility(obj, isShow) {

        if(isShow){
            var msg = $(obj).find("img").attr('alt');
            $(obj).parent().next().children().text(msg);
            $(obj).parent().next().show();
        } else {
            $(obj).parent().next().hide();
        }

    }
