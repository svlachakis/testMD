        function getObjectLabel(objectAPINameString) {
            var objectAPIName = objectAPINameString + '=';
            var obLabel = objectAPINameString;//MD-2942
            if(objectAPIName != '') {

                //MD-1333 Chnanges
                //MD-3737: Start
                if(!vals.includes(' '+objectAPIName) && !vals.includes('{'+objectAPIName)) {
                    return capitalize(objectAPINameString);
                }
                if(vals.includes(' '+objectAPIName)){
                	obLabel = vals.substring(vals.indexOf(' '+objectAPIName));
                }else{
                	obLabel = vals.substring(vals.indexOf('{'+objectAPIName));
                }
                //MD-3737: End

                if(obLabel.includes(',')) {
                    obLabel = (obLabel.substring(0, obLabel.indexOf(', ')).replace(objectAPIName, ''));
                }
                else if(obLabel.includes('}')) {
                    obLabel = (obLabel.substring(0, obLabel.indexOf('}')).replace(objectAPIName, ''));

                }
                if(obLabel.includes("(comma)")){
                    obLabel = obLabel.replace("(comma)",",");
                }

                //MD-3737
                if(obLabel.startsWith('{')) {
                    obLabel = (obLabel.substring(1, obLabel.length - 1));
                }
            }
            return obLabel;
        }

