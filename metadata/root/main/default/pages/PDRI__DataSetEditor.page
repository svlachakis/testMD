	//MD-3395: Method to disable elements while processing in background
	function disableElements(className){

		var eventList = document.getElementsByClassName(className);
		for(var i = 0; i < eventList.length; i++)
			eventList[i].disabled = true;

	}
	//MD-3395: Method to enable elements which are disabled
	function enableElements(className){

		var eventList = document.getElementsByClassName(className);
		for(var i = 0; i < eventList.length; i++)
			eventList[i].disabled = false;
	}

	//MD-3486: Method to prevent governor limit
	function validateSelectionLimit(element, maxLimit, divIdHasSelectedCounting){

		var totalCounting = parseInt(document.getElementById(divIdHasSelectedCounting).innerText);

		if(element.checked ){

			if(totalCounting >= maxLimit){
				alert('{!$Label.WARNING_MAX_EVENT_CONTROL_ITEM}');
				return false;
			}
			document.getElementById(divIdHasSelectedCounting).innerText = totalCounting + 1;

		} else
			document.getElementById(divIdHasSelectedCounting).innerText = totalCounting - 1;

		return true;
	}

