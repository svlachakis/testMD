	//Method to validate inputs and enable invocation entry
	function validateInvocationTemplate(){

		var name = document.getElementById('pg:theForm:invocationTemplateNameInput').value;
		var url = document.getElementById('pg:theForm:invocationTemplateUrlInput').value;

		if(url){

			if(url.includes('http://') || url.includes('https://') || url.includes('salesforce.com')){
				document.getElementById('invocationUrlMessageBox').innerText = '{!$Label.ERROR_DO_NOT_PROVIDE_FULL_URL}';
				url = '';

			}else if(!url.startsWith('/services/apexrest/')){
				document.getElementById('invocationUrlMessageBox').innerText = '{!$Label.ERROR_INVALID_INVOCATION_URL}';
				url = '';
			}else
				document.getElementById('invocationUrlMessageBox').innerText = '';
		} else
			document.getElementById('invocationUrlMessageBox').innerText = '';

		if(name && url){
			showElemVisibility('invocationEntry');
			var elemClass = document.getElementById('invocationEntry').getAttribute('class');
			if( elemClass.indexOf("slds-show") != -1 )
				document.getElementById('invocationAddButton').removeAttribute('disabled');
		} else{

			var elemClass = document.getElementById('invocationEntry').getAttribute('class');
			if( elemClass.indexOf("slds-show") != -1 )
				document.getElementById('invocationAddButton').setAttribute('disabled', 'true');
		}

	}
