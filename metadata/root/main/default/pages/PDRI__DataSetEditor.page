	    function createObjectIcon(objectName, errorMessage, cardIconDivId, cardIconSpanId){ //MD-2713
            var objectNameIcon ;
            var classOnCardIconSpan ;
            //<!-- MD-3266: Static resource updated from SLDS080 to MooverResources -->
            if(errorMessage != ''){
                document.getElementById(cardIconDivId).innerHTML = "<img class=\"slds-icon slds-icon_small\" src=\"{!URLFOR($Resource.MooverResources,'/images/ban_red_60.png')}\" style=\"background-color:white;\" title='"+errorMessage+"' alt=\"Icon\"/>";
